{
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"strict": true,
		"target": "es2020",
		"module": "es2020",
		"lib": ["ES2019", "dom"]
	},
	"angularCompilerOptions": {
		"strictTemplates": true,
		"strictInjectionParameters": true,
	}
}
